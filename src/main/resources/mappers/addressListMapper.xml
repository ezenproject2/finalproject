<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ezen.books.repository.AddressListMapper">

    <select id="getAllAddr" resultType="com.ezen.books.domain.AddressVO">
        select * from address
        where mno = #{mno}
    </select>

    <select id="getMember" resultType="com.ezen.books.domain.MemberVO">
        select * from member
        where mno = #{mno}
    </select>

    <select id="getMemberGrade" resultType="com.ezen.books.domain.GradeVO">
        select * from grade
        where gno = #{gno}
    </select>

</mapper>
